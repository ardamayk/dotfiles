#!/usr/bin/env bash
alias vim="nvim"
alias neovim='sudo -E nvim'
alias ls="eza -l --no-time"
alias lsa="eza -la --no-time"
alias sd="poweroff"
alias pacman="sudo pacman"
alias w="impala"
alias b="bluetui"
alias cd="z"
alias open="xdg-open"
alias wifitui="impala"

alias vlc="flatpak run org.videolan.VLC"
alias telegram="flatpak run org.telegram.desktop"
